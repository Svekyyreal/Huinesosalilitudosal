local Tab = Window:NewTab("CHARACTER")

local Section = Tab:NewSection("Character Editor")

Section:NewButton("Reset","Reser ur character", function()
game.Players.LocalPlayer.Character.Head:Destroy()
end)

Section:NewToggle("Disable Gravity","Gravity Disable", function(state)
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()


if character then
    local humanoid = character:WaitForChild("Humanoid")

    
    humanoid:SetStateEnabled(Enum.HumanoidStateType.Physics, false)
end

end)

Section:NewSlider("Speed Changer","Speed Changer", 1000, 0, function(s) -- 1000 (MaxValue) | 0 (MinValue)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section:NewButton("Reset Speed","Reset Speed", function()
loadstring(game:HttpGet("https://pastebin.com/raw/JPngi4vu"))();
end)

Section:NewSlider("Jump Power","Select Jumppower", 1000, 0, function(s) -- 1000 (MaxValue) | 0 (MinValue)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

Section:NewButton("Reset JumpPower","Reset JumpPower", function()
loadstring(game:HttpGet("https://pastebin.com/raw/Unjpu73W"))();
end)

Section:NewToggle("Infinite Jump", "Lets you infinitely jump", function()
        local InfiniteJumpEnabled = true
        game:GetService("UserInputService").JumpRequest:connect(function()
            if InfiniteJumpEnabled then
                game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
            end
        end)
    end)
    
Section:NewButton("Reset FOV","Reset Camera FOV", function()
loadstring(game:HttpGet("https://pastebin.com/raw/Abfd7NdC"))();
end)

Section:NewTextBox("Change FOV", "Enter new FOV value", function(txt)
    local newFOV = tonumber(txt)
    if newFOV then
        local camera = game.Workspace.CurrentCamera
        if camera then
            camera.FieldOfView = newFOV
        end
    else
        warn("Invalid input. Please enter a valid number for FOV.")
    end
end)

Section:NewButton("Remove Height","Remove Height", function()
loadstring(game:HttpGet("https://pastebin.com/raw/PeUWLes1"))();
end)

Section:NewTextBox("Height Custom","Input height in studs", function(height)
    
    height = tonumber(height)
    
    
    if height then
        
        changeHeight(height)
    else
        
        print("Invalid input. Please enter a number.")
    end
end)

Section:NewTextBox("Spin Speed","Input spin speed", function(speed)
    
    if tonumber(speed) then
        
        spinSpeed = tonumber(speed)
    else
        
        print("Invalid input. Please enter a valid number fr spin speed.")
    end
end)


game:GetService("RunService").Heartbeat:Connect(function()
    
    game.Players.LocalPlayer.Character.HumanoidRootPart.RotVelocity = Vector3.new(0, spinSpeed, 0)
end)

local Section = Tab:NewSection("Other Scripts")

