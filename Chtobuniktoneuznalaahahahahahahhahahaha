local SONG_ID = 6070263388

-- Function to play the song
local function PlaySong()
    local sound = Instance.new("Sound")
    sound.SoundId = "rbxassetid://" .. SONG_ID
    sound.Parent = game.Workspace
    sound:Play()

    -- Fade out the sound gradually over 15 seconds
    game:GetService("TweenService"):Create(sound, TweenInfo.new(15), {Volume = 0}):Play()
end

-- Example condition to start playing the song (replace this with your own condition)
local conditionMet = true  -- Change this to your actual condition

-- Check if the condition is met, then play the song
if conditionMet then
    PlaySong()
end


