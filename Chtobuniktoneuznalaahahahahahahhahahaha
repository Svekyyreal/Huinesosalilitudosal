Logo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Logo.BackgroundTransparency = 1.000
Logo.BorderColor3 = Color3.fromRGB(0, 0, 0)
Logo.BorderSizePixel = 0
Logo.Position = UDim2.new(0.5, 0, 0.5, 0)
Logo.Size = UDim2.new(0, 250, 0, 250)
Logo.Image = "rbxassetid://119927179428964" 
Logo.ImageTransparency = 0.99999999
Logo.ScaleType = Enum.ScaleType.Crop
 
Circle.CornerRadius = UDim.new(110235, 0)
Circle.Name = "Circle"
Circle.Parent = Logo
Circle.Name = "Circle"
Circle.Parent = Logo

-- Scripts:
 
local function ACNFZ_fake_script() -- Intro.Animate 
    local script = Instance.new('LocalScript', Intro)
 
    wait(1)
    local noob = script.Parent.Logo
    Tween = function(Obj, Time, Settings)    
        game:GetService("TweenService"):Create(Obj, TweenInfo.new(Time), Settings):Play()
    end
    Tween(noob, 1.3, {ImageTransparency = 0})
 
    wait(3)
 
    Tween(noob, 1.3, {ImageTransparency = 1})
    wait(1)
 
 
print(juanko) 
end
coroutine.wrap(ACNFZ_fake_script)()
 
wait(5) 
 
local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vdc16Dh7"))()
 
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("ImageLabel")
local TextButton = Instance.new("TextButton")
 
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false


local Frame = Instance.new("ImageLabel")
Frame.Name = "Frame"
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255) -- Background color (not visible due to transparency)
Frame.BackgroundTransparency = 1.000 -- Fully transparent background
Frame.Position = UDim2.new(0.5, -25, 0, 10) -- Centered horizontally, 10 pixels from the top
Frame.Size = UDim2.new(0, 50, 0, 50) -- Size of the frame
Frame.Image = "rbxassetid://119927179428964" -- Replace with your circular image asset ID
Frame.ImageColor3 = Color3.fromRGB(255, 255, 255) -- White color for the image
Frame.ImageTransparency = 0 -- Fully opaque image
Frame.ScaleType = Enum.ScaleType.Slice -- Ensures the image scales properly
Frame.SliceCenter = Rect.new(100, 100, 100, 100) -- Center of the slice (adjust for your image)
Frame.SliceScale = 0.120 -- Scale of the slice (adjust for your image)
Frame.Active = true -- Allows interaction
Frame.ZIndex = 10 -- Ensures it renders above other elements

-- Optional: Add a white outline using a UIStroke
local Outline = Instance.new("UIStroke")
Outline.Parent = Frame
Outline.Color = Color3.fromRGB(255, 255, 255) -- White outline
Outline.Thickness = 2 -- Thickness of the outline
Outline.Transparency = 0 -- Fully opaque outline


TextButton.Parent = Frame
TextButton.AnchorPoint = Vector2.new(0.5, 0.5) -- Change anchor point to center
TextButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- Set text background color to black
TextButton.BackgroundTransparency = 1.000
TextButton.Position = UDim2.new(0.5, 0, 0.5, 0) -- Place text in the center of the frame
TextButton.Size = UDim2.new(1, -10, 1, 0) 
TextButton.Font = Enum.Font.SourceSans
TextButton.Text = ""
TextButton.TextColor3 = Color3.fromRGB(0, 0, 255) -- Change text color to blue
TextButton.TextSize = 20.000
TextButton.TextWrapped = true
TextButton.ZIndex = 11 
TextButton.MouseButton1Down:Connect(function()
    Library:ToggleUI()
end)


local draggingEnabled = false
local dragStartPos = nil
local frameStartPos = nil
 
local function updatePosition(input)
    local delta = input.Position - dragStartPos
    Frame.Position = UDim2.new(
        frameStartPos.X.Scale,
        frameStartPos.X.Offset + delta.X,
        frameStartPos.Y.Scale,
        frameStartPos.Y.Offset + delta.Y
    )
end
 
TextButton.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        draggingEnabled = true
        dragStartPos = input.Position
        frameStartPos = Frame.Position
    end
end)
 
TextButton.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        draggingEnabled = false
    end
end)
 
TextButton.InputChanged:Connect(function(input)
    if draggingEnabled and
        (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        updatePosition(input)
    end
end)
 
Frame.InputChanged:Connect(function(input)
    if draggingEnabled and
        (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        updatePosition(input)
    end
end) 

local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")


local function sendNotification(player, message)
    local icon = "rbxthumb://type=AvatarHeadShot&id=" .. player.UserId .. "&w=150&h=150&fmt=png"
    StarterGui:SetCore("SendNotification", {
        Title = "Notification",
        Text = message,
        Icon = icon,
        Duration = 5
    })
end


local function playerLeft(player)
    sendNotification(player, player.Name .. " left the game.")
end


local function playerJoined(player)
    sendNotification(player, player.Name .. " joined the game.")
end


Players.PlayerRemoving:Connect(playerLeft)
Players.PlayerAdded:Connect(playerJoined)

 
local Window = Library.CreateLib("ArgonFlare | HUB | Version:[BETA]", "DarkTheme")

local Tab = Window:NewTab("MAIN")

local Section = Tab:NewSection("Main") 

Section:NewButton("Rejoin","Rejoin Place",function()
-- Script to log out and rejoin the same server automatically

local Player = game.Players.LocalPlayer

-- Function to log out and rejoin the server
local function logoutAndRejoin()
    -- Get the current place ID (the server's ID)
    local placeId = game.PlaceId
    local jobId = game.JobId

    -- Log the player out (this action depends on the executor used)
    game:GetService("TeleportService"):Teleport(placeId, Player)

    -- Wait a short amount of time before teleporting back to the same server
    wait(3)

    -- Rejoin the same server using the JobId
    game:GetService("TeleportService"):TeleportToPlaceInstance(placeId, jobId, Player)
end

-- Automatically trigger the logout and rejoin process
logoutAndRejoin()
end)

Section:NewButton("Exit","Exit Place",function()
-- Define the delay in seconds before exiting the game
local exitDelay = 5 -- Change this value to adjust the delay (in seconds)

-- Function to exit the game
local function exitGame()
    game:Shutdown() -- Shuts down the game
end

-- Wait for the specified delay before exiting the game
wait(exitDelay)
exitGame() -- Call the function to exit the game
end)

Section:NewButton("Restart", "Restart :)", function()
-- Simple script to kill and respawn the player upon activation

local Player = game.Players.LocalPlayer

-- Function to restart (kill and respawn)
local function restartGame()
    local humanoid = Player.Character:WaitForChild("Humanoid")

    -- Kill the player by setting health to 0
    humanoid.Health = 0

    -- Wait for the player's respawn
    Player.CharacterAdded:Wait()
end

-- Automatically trigger the restart when the script is executed
restartGame()
end)

