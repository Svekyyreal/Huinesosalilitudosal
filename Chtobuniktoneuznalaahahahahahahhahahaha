local Section = Tab:NewSection("Settings")

Section:NewColorPicker("Scheme Color", "Color", Color3.fromRGB(0,0,0), function(color)
    local colors = {
    SchemeColor = Color3.fromRGB(0,255,255),
}
    
end)

Section:NewColorPicker("Background Color", "Color", Color3.fromRGB(0,0,0), function(color)
local colors = {
    Background = Color3.fromRGB(0, 0, 0),
}
end)

Section:NewColorPicker("Header", "Color", Color3.fromRGB(0,0,0), function(color)
local colors = {
Header = Color3.fromRGB(0, 0, 0),
}
end)

Section:NewColorPicker("Color Text", "Color", Color3.fromRGB(0,0,0), function(color)
local colors = {
    TextColor = Color3.fromRGB(255,255,255),
}
end)

Section:NewColorPicker("Element Color", "Color Info", Color3.fromRGB(0,0,0), function(color)
local colors = {
ElementColor = Color3.fromRGB(20, 20, 20)
}
end)

