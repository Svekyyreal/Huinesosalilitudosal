loadstring(game:HttpGet("https://raw.githubusercontent.com/zephyr10101/ignore-touchinterests/main/main",true))()
end)

Section:NewToggle("Xray", "Xray", function(state)
    if state then
        	transparent = true
	        x(transparent)
    else
        	transparent = false
	        x(transparent)
    end
end)

Section:NewButton("Jump", "Jump Button", function()
    -- Get the player's character
    local character = game.Players.LocalPlayer.Character
    
    -- Check if the character exists and is not jumping already
    if character and not character.Humanoid:GetState() == Enum.HumanoidStateType.Jumping then
        -- Call the Jump method of the Humanoid to make the character jump
        character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

Section:NewButton("Sit", "Button Sit", function()
    -- Get the player's character
    local character = game.Players.LocalPlayer.Character
    
    -- Check if the character exists and is not sitting already
    if character and not character.Humanoid.Sit then
        -- Set the Sit property of the Humanoid to true to make the character sit
        character.Humanoid.Sit = true
    end
end)

Section:NewButton("Lay Down", "Lay", function()
    game:GetService("Players").LocalPlayer.Character:Move(Vector3.new(0, -5, 0))
end)

